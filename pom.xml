<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.github.to2mbn</groupId>
	<artifactId>jmccc</artifactId>
	<version>2.0-SNAPSHOT</version>
	<name>JMCCC</name>
	<description>An open-source lightweight library for launching Minecraft.</description>
	<url>https://github.com/to2mbn/JMCCC</url>
	<issueManagement>
		<url>https://github.com/to2mbn/JMCCC/issues</url>
		<system>GitHub Issues</system>
	</issueManagement>
	<licenses>
		<license>
			<name>MIT License</name>
			<url>http://www.opensource.org/licenses/mit-license.php</url>
			<distribution>repo</distribution>
		</license>
	</licenses>
	<scm>
		<url>https://github.com/to2mbn/JMCCC</url>
		<connection>scm:git:git://github.com/to2mbn/JMCCC.git</connection>
		<developerConnection>scm:git:git@github.com:to2mbn/JMCCC.git</developerConnection>
	</scm>
	<ciManagement>
		<system>Jenkins</system>
		<url>http://ci.infinity-studio.org/job/JMCCCv2/</url>
	</ciManagement>
	<developers>
		<developer>
			<email>2569766005@qq.com</email>
			<name>Qiyun Zhou</name>
			<url>https://github.com/darkyoooooo</url>
			<id>darkyoooooo</id>
		</developer>
		<developer>
			<email>yushijinhun@gmail.com</email>
			<name>Haowei Wen</name>
			<url>https://github.com/yushijinhun</url>
			<id>yushijinhun</id>
		</developer>
	</developers>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.3</version>
				<configuration>
					<source>1.7</source>
					<target>1.7</target>
					<encoding>UTF8</encoding>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<dependencies>
		<dependency>
			<groupId>com.google.code.gson</groupId>
			<artifactId>gson</artifactId>
			<version>2.3.1</version>
		</dependency>
	</dependencies>
</project>
